      "source": [
        "def count_words(sentence):\n",
        "    words = sentence.strip().split()\n",
        "    return len(words)\n",
        "\n",
        "def main():\n",
        "    text = \"GitHub helps developers share code\"\n",
        "    print(\"Sentence:\", text)\n",
        "    print(\"Word count:\", count_words(text))\n",
        "\n",
        "if __name__ == \"__main__\":\n",
        "    main()"
      ]
    }
  ]
